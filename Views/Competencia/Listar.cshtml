@model ListaCompetenciaViewModel
<h1>Listando CompetÃªncias</h1>
<table>   
     <tr>
        <th>Id</th>
        <th>Coluna Bloom</th>
        <th>Linha Bloom</th> 
        <th>Apagar</th>  
     </tr>
     @{
       foreach(var item in Model.Competencias)
       {
          <tr>
              <td>@item.Id</td>
              <td>@item.ColunaBloom</td>
              <td>@item.LinhaBloom</td>
              <td><a href="/Competencia/Deletar/@item.Id">Deletar</a></td>
          </tr>

       } 
     }
</table>